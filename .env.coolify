# =============================================================================
# Google Maps Scraper - Environment Variables for Coolify
# =============================================================================
# Generated on: 2025-07-18T17:16:04.157Z
# Copy these values to your Coolify environment variables section
# =============================================================================

# Node.js environment
NODE_ENV=production

# Server port (Coolify will override this automatically)
PORT=3000

# API authentication key (optional - leave empty to disable)
API_KEY=

# Maximum number of results to scrape per job
MAX_RESULTS=100

# Number of results to process in each batch (1-5 recommended)
BATCH_SIZE=3

# Stealth level: optimized (faster) or high-stealth (more anti-detection)
STEALTH_LEVEL=optimized

# Maximum requests per minute
REQUESTS_PER_MINUTE=60

# Delay between requests in milliseconds
DELAY_BETWEEN_REQUESTS=1000

# Maximum concurrent scraping jobs
MAX_CONCURRENT_JOBS=2

# Enable proxy usage (true/false)
USE_PROXY=false

# Comma-separated list of proxy URLs
PROXY_LIST=

# Browser to use: chromium, firefox, webkit
BROWSER=chromium

# Run browser in headless mode (always true for production)
HEADLESS=true

# CAPTCHA solving service API key (optional)
CAPTCHA_API_KEY=

# CAPTCHA service: 2captcha, anticaptcha, capmonster
CAPTCHA_SERVICE=

# SQLite database path
DB_PATH=./data/scraper.db

# Enable email notifications
EMAIL_ENABLED=false

# SMTP server host
EMAIL_HOST=smtp.gmail.com

# SMTP server port
EMAIL_PORT=587

# SMTP username
EMAIL_USER=

# SMTP password
EMAIL_PASS=

# From email address
EMAIL_FROM=

# To email address
EMAIL_TO=

# Log level: error, warn, info, debug
LOG_LEVEL=info

# Enable detailed logging for debugging
DEBUG=false

# Trust proxy headers (set to true if behind reverse proxy)
TRUST_PROXY=false

# Session secret for secure cookies (auto-generated)
SESSION_SECRET=bda328fb7d3e44f78a90a74bcaaf9f95d12800507a62d22e2a97c0ee98b420bb

